      <para id="id5393811">(c) Nee</para>
      <para id="id1168889339197">(d) Ja</para>
      <para id="id1168889651268">(e) Nee</para>
      <para id="id1168889340535">(f) Ja</para>
      <para id="id7156655">(g) Ja</para>
      <para id="id2275857">(h) Nee</para>
