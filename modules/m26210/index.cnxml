          </media>
        </figure>
        <para id="id1169616445878"> a) b)</para>
        <figure id="id8160378">
          <media id="id8160378_media" alt="">
            <image mime-type="image/png" src="../../media/graphics8-add6.png" id="id8160378__onlineimage" height="312" width="590"/>
          </media>
        </figure>
        <para id="id7433714"> c) d)</para>
        <figure id="id8050002">
          <media id="id8050002_media" alt="">
            <image mime-type="image/png" src="../../media/graphics9-add6.png" id="id8050002__onlineimage" height="307" width="590"/>
          </media>
